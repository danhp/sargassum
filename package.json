{
  "name": "sargassum",
  "productName": "Sargassum",
  "version": "0.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "electronVersion": "0.35.1",
  "scripts": {
    "test": "xo",
    "start": "electron .",
    "build-osx": "electron-packager . $npm_package_productName --overwrite --out=dist --ignore='^/dist$' --ignore='^/imgs$' --prune --platform=darwin --arch=x64 --icon=imgs/icon.icns --app-version=$npm_package_version --version=$npm_package_electronVersion"
  },
  "devDependencies": {
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.35.1",
    "xo": "*"
  },
  "dependencies": {
    "electron-gh-releases": "^2.0.1",
    "xo": "*"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  }
}
